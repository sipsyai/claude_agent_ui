{
  "name": "claude-agent-ui",
  "version": "1.0.0",
  "type": "module",
  "description": "Web UI for managing Claude agents, skills, slash commands, and MCP servers",
  "main": "dist/server.js",
  "keywords": [
    "claude",
    "claude-agent-sdk",
    "ai",
    "web-ui",
    "agent",
    "mcp",
    "agent-management"
  ],
  "author": "Ali",
  "license": "MIT",
  "engines": {
    "node": ">=20.19.0"
  },
  "scripts": {
    "dev": "concurrently \"npm run dev:server\" \"npm run dev:frontend\"",
    "dev:server": "cross-env NODE_ENV=development PORT=3001 tsx watch src/server.ts",
    "dev:frontend": "cross-env NODE_ENV=development vite",
    "dev:web": "cross-env NODE_ENV=development vite",
    "clean": "rimraf dist",
    "build": "npm run clean && npm run build:frontend && npm run build:server",
    "build:frontend": "cross-env NODE_ENV=production vite build",
    "build:server": "tsc && tsc-alias",
    "start": "cross-env NODE_ENV=production PORT=3001 node dist/server.js",
    "typecheck": "tsc --noEmit",
    "test": "bash tests/run-all-tests.sh",
    "test:e2e": "bash tests/e2e/test-runner.sh",
    "test:performance": "bash tests/performance/benchmark.sh",
    "test:security": "bash tests/security/security-audit.sh",
    "analyze-migration": "tsx project_migration/analyze-with-skills.ts",
    "analyze:postgresql": "tsx project_migration/analyze-postgresql.ts",
    "analyze:typescript": "tsx project_migration/analyze-typescript.ts",
    "analyze:express": "tsx project_migration/analyze-express.ts",
    "analyze:docker": "tsx project_migration/analyze-docker.ts",
    "migrate": "tsx scripts/migration-tools/migrate-sqlite-to-postgres.ts",
    "migrate:validate": "tsx scripts/migration-tools/migrate-sqlite-to-postgres.ts --validate-only",
    "migrate:skip-backup": "tsx scripts/migration-tools/migrate-sqlite-to-postgres.ts --skip-backup",
    "migrate:markdown": "tsx scripts/migrate-from-markdown.ts",
    "validate-migration": "tsx scripts/migration-tools/validate-migration.ts",
    "rollback-migration": "tsx scripts/migration-tools/rollback-migration.ts",
    "test:rollback": "tsx scripts/migration-tools/test-rollback-procedure.ts",
    "test:health": "bash scripts/test-health-endpoints.sh",
    "verify:tables": "tsx scripts/verify-postgres-tables.ts",
    "import-training-agent": "tsx scripts/import-training-agent.ts",
    "run-payload": "tsx run-sdk-payload.ts"
  },
  "dependencies": {
    "@anthropic-ai/claude-agent-sdk": "^0.1.42",
    "@anthropic-ai/sdk": "^0.54.0",
    "@assistant-ui/react": "^0.11.35",
    "@assistant-ui/react-markdown": "^0.11.3",
    "@modelcontextprotocol/sdk": "^1.20.2",
    "@musistudio/llms": "^1.0.38",
    "@radix-ui/react-checkbox": "^1.3.2",
    "@radix-ui/react-dialog": "^1.1.14",
    "@radix-ui/react-label": "^2.1.7",
    "@radix-ui/react-popover": "^1.1.14",
    "@radix-ui/react-select": "^2.2.5",
    "@radix-ui/react-slot": "^1.2.3",
    "@radix-ui/react-switch": "^1.2.5",
    "@radix-ui/react-tabs": "^1.1.12",
    "@radix-ui/react-tooltip": "^1.2.7",
    "@types/better-sqlite3": "^7.6.13",
    "@types/eventsource": "^1.1.15",
    "@types/react-syntax-highlighter": "^15.5.13",
    "@types/web-push": "^3.6.4",
    "axios": "^1.13.1",
    "better-sqlite3": "^12.4.1",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "cors": "^2.8.5",
    "diff": "^8.0.2",
    "eventsource": "^4.0.0",
    "express": "^4.18.2",
    "gray-matter": "^4.0.3",
    "js-yaml": "^4.1.0",
    "lru-cache": "^11.2.2",
    "lucide-react": "^0.536.0",
    "multer": "^2.0.2",
    "pino": "^8.17.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-markdown": "^10.1.0",
    "react-router-dom": "^6.22.0",
    "react-syntax-highlighter": "^16.1.0",
    "tailwind-merge": "^3.3.1",
    "uuid": "^11.1.0",
    "web-push": "^3.6.7",
    "zod": "^3.25.76"
  },
  "devDependencies": {
    "@tailwindcss/typography": "^0.5.16",
    "@tailwindcss/vite": "^4.1.11",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/js-yaml": "^4.0.9",
    "@types/multer": "^2.0.0",
    "@types/node": "^20.19.1",
    "@types/react": "^18.2.66",
    "@types/react-dom": "^18.2.22",
    "@types/uuid": "^9.0.7",
    "@vitejs/plugin-react": "^4.2.1",
    "autoprefixer": "^10.4.21",
    "concurrently": "^8.2.2",
    "cross-env": "^10.1.0",
    "postcss": "^8.5.6",
    "rimraf": "^6.0.1",
    "tailwindcss": "^4.1.11",
    "tsc-alias": "^1.8.16",
    "tsx": "^4.6.2",
    "typescript": "^5.3.3",
    "vite": "^7.0.6"
  },
  "optionalDependencies": {
    "@tailwindcss/oxide-linux-x64-gnu": "^4.1.11",
    "@tailwindcss/oxide-win32-x64-msvc": "^4.1.16",
    "lightningcss-linux-x64-gnu": "^1.30.1",
    "lightningcss-win32-x64-msvc": "^1.30.2"
  }
}
